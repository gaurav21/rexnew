}

.btn-primary:active,
.btn-primary.active {
    background-color: #b6b6b6;
    border-color: #b6b6b6;
}

.btn-success, .btn-success:hover, .btn-success:focus, .btn-success:active {
    background:none;
    border-color: #fcfcfc;
    border-radius:4px;
    width:241px; height:42px;

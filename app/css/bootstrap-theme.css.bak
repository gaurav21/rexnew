    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffffff', endColorstr='#ffe6e6e6', GradientType=0);
}

.btn-default:active,
.btn-default.active {
    background-color: #e6e6e6;
    border-color: #e0e0e0;
}

.btn-primary {
    background:#b6b6b6;
    border-radius:4px;
    font-family:Proximanovabold;
    color:#444444;
    font-size:14px;
    margin:22px 25px;

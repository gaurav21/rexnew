    color:#fff;
    margin:15px 0 0 12px;
}

.btn-success:active,
.btn-success.active {
    background:none;
    border-color: #fcfcfc;
}

.btn-warning {
    background-image: -webkit-gradient(linear, left 0%, left 100%, from(#f0ad4e), to(#ec971f));
